<% if @flag %>
  window.location.href = "/ofertas"
<% else %>
  $(".sessions-errors").html("<%= escape_javascript(render("errors")) %>");
  $("#signin_form")[0].reset();
  $(document).ready(function() {
    setTimeout(function() {
      $(".sessions-errors .devise-errors").slideUp();
    }, 8000);
  });
<% end %>  